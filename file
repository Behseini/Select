function mover() {
    var parent = $(this).data('trigger');
    //    setTimeout(function(){
    $("#" + parent).click();
    // $("#"+parent).removeClass('active');
    $('.navbar-nav .nav-link').removeClass('active');

    //    }, 120);

}

$('.nav-link').on("click", function () {
    if ($(this).hasClass('active')) {
        setTimeout(function () {
            $('.nav-link').removeClass('active');
        }, 100);
    }

});


$(".accordion .drop-card").hoverIntent({
    out: mover
});

