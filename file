});

$("#show-mobile-menu").on("click", function () {
    $(this).find('span').toggleClass('navbar-toggler-menu-on');
    $("#mobileNavbarPromotions").collapse('hide');
});


$(document).on("click", "#sidebar-nav-forgot-tab", function(){
    $('#user-login-sidebar').addClass("d-none");
    $('#user-retrive-sidebar').removeClass('d-none');
    $('#forgot-parag').removeClass("d-none");
    $('#pass-parag').addClass("d-none");
});

